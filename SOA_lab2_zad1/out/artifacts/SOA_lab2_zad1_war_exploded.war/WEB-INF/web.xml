<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>srednia_servlet</servlet-name>
        <servlet-class>srednia_servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>srednia_servlet</servlet-name>
        <url-pattern>/srednia_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>posortuj_servlet</servlet-name>
        <servlet-class>posortuj_servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>posortuj_servlet</servlet-name>
        <url-pattern>/posortuj_servlet</url-pattern>
    </servlet-mapping>
</web-app>